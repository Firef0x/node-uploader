{
  "name": "node-uploader",
  "description": "Node.js based file uploader",
  "version": "0.1.1",
  "homepage": "https://github.com/Firef0x/node-uploader",
  "main": "src/www",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/Firef0x/node-uploader.git"
  },
  "author": "Firef0x",
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "bugs": {
    "url": "https://github.com/Firef0x/node-uploader/issues"
  },
  "scripts": {
    "build": "npm run clean && npm run copyStatic && cross-env NODE_ENV=production webpack --progress --config config/webpack.prod.js",
    "clean": "rimraf public",
    "copyStatic": "cpx \"static/**/*.*\" public/",
    "dev": "npm run copyStatic && cross-env NODE_ENV=development NPCONFIG=dev node --inspect src/www",
    "lint": "eslint --ext .js,.jsx -f ./node_modules/eslint-html-reporter/reporter.js -o lint-report.html client-react src stories",
    "start": "cross-env NODE_ENV=production node src/www",
    "startProd": "npm run copyStatic && cross-env NODE_ENV=production node src/www",
    "test": "node config/test.js",
    "test:mocha": "cross-env FORCE_COLOR=true NODE_ENV=development NPCONFIG=dev mocha -t 120000 --reporter mochawesome --watch src/**/*test.js",
    "test:cover": "npm test -- --coverage",
    "test:update": "npm test -- --updateSnapshot",
    "storybook": "npx start-storybook -p 9009",
    "storybook:build": "npx build-storybook -o docs/storybook",
    "storybook:run": "npx http-server ./docs/storybook -s -o -p 9009 -e html"
  },
  "browserslist": [
    "> 0.2%",
    "not dead",
    "not ie <= 8",
    "not op_mini all"
  ],
  "dependencies": {
    "antd": "^3.25.2",
    "express": "^4.17.1",
    "hbs": "^4.0.6",
    "multer": "^1.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@storybook/addon-actions": "^4.0.9",
    "@storybook/addon-info": "^4.1.3",
    "@storybook/addon-knobs": "^4.0.12",
    "@storybook/addon-options": "^4.1.3",
    "@storybook/addons": "^4.0.9",
    "@storybook/react": "^4.0.9",
    "autoprefixer": "^9.7.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-jest": "^24.1.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-html-reporter": "^0.7.4",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "husky": "^1.3.1",
    "jest": "24.9.0",
    "jest-enzyme": "^7.1.2",
    "jest-html-reporter": "^2.5.0",
    "jsdom": "^16.5.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^5.2.0",
    "mochawesome": "^3.1.1",
    "mz": "^2.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.12.17",
    "rimraf": "^3.0.0",
    "style-loader": "^1.0.0",
    "supertest": "^3.4.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}

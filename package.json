{
  "name": "node-uploader",
  "description": "Node.js based file uploader",
  "version": "0.1.0",
  "homepage": "https://github.com/Firef0x/node-uploader",
  "main": "src/www",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/Firef0x/node-uploader.git"
  },
  "author": "Firef0x",
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 4.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run copyStatic && cross-env NODE_ENV=production webpack --progress --config config/webpack.prod.js",
    "clean": "rimraf public",
    "copyStatic": "cpx \"static/**/*.*\" public/",
    "dev": "npm run copyStatic && cross-env NODE_ENV=development NPCONFIG=dev node --inspect src/www",
    "lint": "eslint --ext .js,.jsx client-react src stories",
    "start": "cross-env NODE_ENV=production node src/www",
    "startProd": "npm run copyStatic && cross-env NODE_ENV=production node src/www",
    "test": "node config/test.js",
    "test:mocha": "cross-env FORCE_COLOR=true NODE_ENV=development NPCONFIG=dev mocha -t 120000 --reporter mochawesome --watch src/**/*test.js",
    "test:cover": "npm test -- --coverage",
    "test:update": "npm test -- --updateSnapshot",
    "storybook": "npx start-storybook -p 9009",
    "storybook:build": "npx build-storybook -o docs/storybook",
    "storybook:run": "npx http-server ./docs/storybook -s -o -p 9009 -e html"
  },
  "browserslist": [
    "> 0.2%",
    "not dead",
    "not ie <= 8",
    "not op_mini all"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@feathersjs/express": "^1.2.7",
    "@feathersjs/feathers": "^3.2.3",
    "antd": "3.13.5",
    "hbs": "^4.0.1",
    "multer": "^1.4.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@storybook/addon-actions": "^4.0.9",
    "@storybook/addon-info": "^4.1.3",
    "@storybook/addon-knobs": "^4.0.12",
    "@storybook/addon-options": "^4.1.3",
    "@storybook/addons": "^4.0.9",
    "@storybook/react": "^4.0.9",
    "autoprefixer": "^9.4.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.1.0",
    "babel-loader": "^8.0.4",
    "babel-preset-jest": "^24.1.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "0.11.1",
    "husky": "^1.3.1",
    "jest": "24.1.0",
    "jest-enzyme": "^7.0.1",
    "jest-html-reporter": "2.4.4",
    "jsdom": "^13.1.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "lodash.foreach": "^4.5.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "mochawesome": "^3.1.1",
    "mz": "^2.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.6.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.1",
    "supertest": "^3.4.2",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  }
}
